# BudgetTrak Environment Configuration
# Copy this to .env and fill in your values

# Gemini API Key (get from: https://aistudio.google.com/apikey)
GEMINI_API_KEY=your_gemini_api_key_here

# Google Credentials (download from Google Cloud Console)
GOOGLE_CREDENTIALS_PATH=budgetrak_credentials.json

# Google Sheet ID (create sheet and paste ID here)
BUDGET_SHEET_ID=your_google_sheet_id_here

# Gemini Model
GEMINI_MODEL=gemini-2.5-flash
GEMINI_TEMPERATURE=0.1

# Debug Mode
DEBUG=false
